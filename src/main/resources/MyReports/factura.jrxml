	<queryString language="SQL">
		<![CDATA[select id, concat(pNombre, " ", pApellido) as nombreProducto, cantidad, subtotal,
cantidad * subtotal as total from facturasProductos as fp inner join facturas as f on 
fp.idFactura = f.idFactura inner join usuarios as us on us.idUsuario = f.idUsuario
inner join productos as pr on pr.idProducto = fp.idProducto]]>
	</queryString>
